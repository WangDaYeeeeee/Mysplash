<?xml version="1.0" encoding="utf-8"?>
<com.wangdaye.mysplash._common.ui.widget.SwipeBackLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_about_swipeBackLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.wangdaye.mysplash._common.ui.widget.coordinatorView.StatusBarView
        android:id="@+id/activity_about_statusBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimaryDark" />

    <android.support.design.widget.CoordinatorLayout
        android:id="@+id/activity_about_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?attr/root_color">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/activity_about_recyclerView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </android.support.design.widget.CoordinatorLayout>

</com.wangdaye.mysplash._common.ui.widget.SwipeBackLayout>